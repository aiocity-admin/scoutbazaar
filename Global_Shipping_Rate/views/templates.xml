<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="displaiy_rate_card">
        <title>Shipping Rate Card</title>
        <t t-if="show_rate_card == True">
            <div >
                <t t-foreach="shipping_rates" t-as="s_rate">
                    <p>
               Name : <t t-esc="s_rate.name"/>
                    </p>
                    <p>
               Source country : <t t-esc="s_rate.source_country.name"/>
                    </p>
                    <p>
               Destination country : <t t-esc="s_rate.destination_country.name"/>
                    </p>
                </t>
            </div>
        </t>
        <t t-if="show_rate_card == False">
            <div style="color: red;">Error Message : <t t-esc="error_msg"/>
            </div>
        </t>
    </template>
</odoo>