<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="new_web_page">
        <title>Student List</title>
        <t t-foreach="students" t-as="student">
            <p>
                <t t-esc="student.name"/>
            </p>
        </t>
        <div style="color: red;">Source country : <t t-esc="source_country"/></div>
        <div style="color: red;">Destination Country : <t t-esc="destination_country"/></div>
        <div style="color: red;">Package Weight : <t t-esc="package_weight"/>gm</div>
         <div style="color: red;">Shipping Cost : <t t-esc="package_weight"/>gm</div>
</template>
</odoo>