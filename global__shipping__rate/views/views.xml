<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="global__shipping__rate.list">
      <field name="name">Global_Shipping_Rate list</field>
      <field name="model">global__shipping__rate.global__shipping__rate</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="value"/>
          <field name="value2"/>
        </tree>
      </field>
    </record>

    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="global__shipping__rate.action_window">
      <field name="name">Global_Shipping_Rate window</field>
      <field name="res_model">global__shipping__rate.global__shipping__rate</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->

    <record model="ir.actions.server" id="global__shipping__rate.action_server">
      <field name="name">Global_Shipping_Rate server</field>
      <field name="model_id" ref="model_global__shipping__rate_global__shipping__rate"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>


    <!-- Top menu item -->

    <menuitem name="Global_Shipping_Rate" id="global__shipping__rate.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Menu 1" id="global__shipping__rate.menu_1" parent="global__shipping__rate.menu_root"/>
    <menuitem name="Menu 2" id="global__shipping__rate.menu_2" parent="global__shipping__rate.menu_root"/>

    <!-- actions -->

    <menuitem name="List" id="global__shipping__rate.menu_1_list" parent="global__shipping__rate.menu_1"
              action="global__shipping__rate.action_window"/>
    <menuitem name="Server to list" id="global__shipping__rate" parent="global__shipping__rate.menu_2"
              action="global__shipping__rate.action_server"/>

  </data>
</odoo>